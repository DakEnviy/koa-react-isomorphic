[ignore]
.*/build/.*
.*/config/.*
.*/node_modules/fbjs/.*
.*/node_modules/nock/.*
.*/test/.*

[include]

[libs]

[options]
module.system=haste

# alias for babel-plugin-module-alias
module.name_mapper='^app\/\(.*\)$' -> '<PROJECT_ROOT>/app/\1'

module.name_mapper='.*\.\(css\|scss\)$' -> '<PROJECT_ROOT>/app/flow-interfaces/css-modules.js.flow'
module.name_mapper='.*\.\(svg\|png\|jpg\|jpeg\|gif\)$' -> '<PROJECT_ROOT>/app/flow-interfaces/webpack-assets.js.flow'

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe